[
  {
    "key": "space",
    "command": "npm.focus",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'",
  },
  {
    "key": "tab",
    "command": "search.action.focusNextSearchResult",
    "when": "inSearchEditor"
  },
  {
    "key": ", q",
    "command": "search.action.openNewEditor",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "enter",
    "command": "workbench.action.toggleEditorWidths",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", t",
    "command": "workbench.explorer.fileView.focus",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", t",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "explorerViewletVisible && filesExplorerFocus"
  },
  {
    "key": ", q",
    "command": "rest-client.request",
    "when": "editorTextFocus && editorLangId == 'http'"
  },
  {
    "key": ", w",
    "command": "code-runner.run",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", k",
    "command": "workbench.action.moveEditorRightInGroup",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", j",
    "command": "workbench.action.moveEditorLeftInGroup",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", c",
    "command": "copyFilePath",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", v",
    "command": "copy-relative-path-and-line-numbers.both",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", e",
    "command": "editor.action.marker.nextInFiles",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", x",
    "command": "workbench.action.closeAllEditors",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", p",
    "command": "workbench.action.pinEditor",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", a",
    "command": "editor.action.formatChanges",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", d",
    "command": "workbench.action.closeActiveEditor",
    "when": "vim.active && vim.mode == 'Normal' || !editorTextFocus"
  },
  {
    "key": ", d",
    "command": "workbench.action.closeActivePinnedEditor",
    "when": "vim.active && vim.mode == 'Normal' || !editorTextFocus"
  },
  {
    "key": ", f",
    "command": "workbench.action.quickOpen",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", m",
    "command": "bookmarks.toggle",
    "when": "vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", z",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "vim.active && vim.mode == 'Normal'"
  },
  {
    "key": ", s",
    "command": "workbench.action.splitEditorRight",
    "when": "vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "3",
    "command": "bookmarks.jumpToNext",
    "when": "vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "cmd+enter",
    "command": "renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus"
  },
  {
    "key": "enter",
    "command": "-renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus"
  },
  {
    "key": "enter",
    "command": "list.select",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "[",
    "command": "workbench.action.focusNextGroup",
    "when": "vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "\\",
    "command": "workbench.action.terminal.focus",
    "when": "vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "\\",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "\\",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "listFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "tab",
    "command": "workbench.action.nextEditorInGroup",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "backspace",
    "command": "workbench.action.previousEditorInGroup",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "shift+cmd+-",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "shift+cmd+-",
    "command": "editor.foldAll",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+0",
    "command": "-editor.foldAll",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+-",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "cmd+-",
    "command": "editor.fold",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+cmd+[",
    "command": "-editor.fold",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+=",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "shift+cmd+=",
    "command": "editor.unfoldAll",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+j",
    "command": "-editor.unfoldAll",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+=",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "cmd+=",
    "command": "editor.unfold",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+cmd+]",
    "command": "-editor.unfold",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+\\",
    "command": "workbench.action.closeAllGroups"
  },
  {
    "key": "cmd+k shift+cmd+w",
    "command": "-workbench.action.closeAllGroups"
  },
  {
    "key": "ctrl+\\",
    "command": "workbench.action.closeAllEditors"
  },
  {
    "key": "cmd+k cmd+w",
    "command": "-workbench.action.closeAllEditors"
  },
  {
    "key": "cmd+l",
    "command": "workbench.action.gotoLine"
  },
  {
    "key": "ctrl+g",
    "command": "-workbench.action.gotoLine"
  },
  {
    "key": "cmd+p",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "shift+",
    "command": "workbench.action.quickOpenNavigateNextInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  {
    "key": "cmd+p",
    "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  {
    "key": "shift+",
    "command": "workbench.action.quickOpenRecent"
  },
  {
    "key": "cmd+p",
    "command": "workbench.action.gotoSymbol"
  },
  {
    "key": "shift+cmd+o",
    "command": "-workbench.action.gotoSymbol"
  },
  {
    "key": "alt+down",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "cmd+t",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+`",
    "command": "workbench.actions.view.problems"
  },
  {
    "key": "shift+cmd+m",
    "command": "-workbench.actions.view.problems"
  },
  {
    "key": "cmd+j",
    "command": "-workbench.action.togglePanel"
  },
  {
    "key": "cmd+j",
    "command": "findJump.activate"
  },
  {
    "key": "shift+cmd+h",
    "command": "-workbench.action.replaceInFiles"
  },
  {
    "key": "cmd+r",
    "command": "editor.action.startFindReplaceAction"
  },
  {
    "key": "alt+cmd+f",
    "command": "-editor.action.startFindReplaceAction"
  },
  {
    "key": "ctrl+shift+`",
    "command": "-workbench.action.terminal.new"
  },
  {
    "key": "cmd+t",
    "command": "-workbench.action.showAllSymbols"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+b",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "cmd+;",
    "command": "outline.focus"
  },
  {
    "key": "ctrl+=",
    "command": "editor.action.fontZoomIn"
  },
  {
    "key": "ctrl+0",
    "command": "-workbench.action.lastEditorInGroup"
  },
  {
    "key": "ctrl+0",
    "command": "editor.action.fontZoomReset"
  },
  {
    "key": "alt+j",
    "command": "findJump.activateWithSelection"
  },
  {
    "key": "cmd+i",
    "command": "-expandLineSelection",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+m",
    "command": "workbench.action.toggleZenMode"
  },
  {
    "key": "cmd+k z",
    "command": "-workbench.action.toggleZenMode"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.navigateBack"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.quickInputBack",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+-",
    "command": "editor.action.fontZoomOut"
  },
  {
    "key": "f4",
    "command": "-search.action.focusNextSearchResult",
    "when": "hasSearchResult || inSearchEditor"
  }
]